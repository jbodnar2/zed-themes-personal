{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "name": "Vajra Zenith",
  "author": "Jon Bodnar",
  "themes": [
    {
      "name": "Vajra Zenith",
      "appearance": "dark",
      "description": "A deep purple-themed editor with cyan accents, optimized for focus and readability. Fully WCAG AA compliant with distinct semantic colors and keyboard navigation support.",
      "keywords": [
        "purple",
        "cyan",
        "dark",
        "zenith",
        "vajra",
        "accessibility"
      ],
      "version": "1.0.0",
      "style": {
        "background": "#1a1828ff",
        "surface.background": "#211f32ff",
        "elevated_surface.background": "#2a2740ff",
        "panel.background": "#1a1828ff",
        "editor.background": "#211f32ff",
        "editor.gutter.background": "#211f32ff",
        "editor.subheader.background": "#1a1828ff",

        "border": "#2a2740ff",
        "border.focused": "#60cfffff",
        "border.selected": "#60cfffff",
        "border.transparent": "#00000000",
        "border.variant": "#60cfff66",

        "conflict": "#c79ceaff",
        "conflict.background": "#302b4544",
        "conflict.border": "#c79ceaff",

        "created": "#3a8abfff",
        "created.background": "#25334044",
        "created.border": "#3a8abfff",

        "deleted": "#c79ceaff",
        "deleted.background": "#302b4544",
        "deleted.border": "#c79ceaff",

        "drop_target.background": "#60cfff44",

        "error": "#ff7b7bff",
        "error.background": "#442a2a66",
        "error.border": "#ff7b7bff",

        "ignored": "#6e6a86ff",
        "ignored.background": "#2b284844",
        "ignored.border": "#6e6a8666",

        "info": "#60cfff",
        "info.background": "#25384a44",
        "info.border": "#60cfff",

        "modified": "#ffca80ff",
        "modified.background": "#453a2a44",
        "modified.border": "#ffca80ff",

        "warning": "#ffca80ff",
        "warning.background": "#453a2a44",
        "warning.border": "#ffca80ff",

        "pane_group.border": "#2a2740ff",

        "status_bar.background": "#1a1828ff",
        "title_bar.background": "#1a1828ff",
        "title_bar.inactive_background": "#211f32ff",
        "toolbar.background": "#1a1828ff",

        "text": "#f0edffff",
        "text.accent": "#60cfff",
        "text.disabled": "#6e6a86ff",
        "text.muted": "#b5bae6ff",
        "text.placeholder": "#9a95bbff",

        "focus.border": "#60cfffff",
        "focus.background": "#60cfff22",
        "focus.highlight": "#60cfffff",

        "element.active": "#60cfff66",
        "element.background": "#2a274044",
        "element.disabled": "#00000000",
        "element.hover": "#60cfff44",
        "element.selected": "#60cfff55",

        "ghost_element.active": "#60cfff66",
        "ghost_element.background": "#00000000",
        "ghost_element.disabled": "#00000000",
        "ghost_element.hover": "#60cfff44",
        "ghost_element.selected": "#60cfff55",

        "tab.active_background": "#2a2740ff",
        "tab.inactive_background": "#1a1828ff",
        "tab_bar.background": "#1a1828ff",

        "editor.active_line.background": "#60cfff22",
        "editor.active_wrap_guide": "#5a5573cc",
        "editor.document_highlight.read_background": "#60cfff22",
        "editor.document_highlight.write_background": "#ffca8022",
        "editor.foreground": "#f0edffff",
        "editor.highlighted_line.background": "#2a2740ff",
        "editor.hover_line_number": "#60cfffff",
        "editor.invisible": "#5a5573aa",
        "editor.line_number": "#7a759aff",
        "editor.active_line_number": "#b5bae6ff",
        "editor.wrap_guide": "#5a5573aa",
