        "editor.indent_guide_active": "#60cfff99",
        "editor.selection": "#60cfff33",
        "editor.selection.border": "#60cfff00",
        "editor.selection.highlight": "#60cfff44",
        "editor.selection.highlight.border": "#60cfff00",

        "list.selection": "#60cfff22",
        "list.selection.border": "#60cfff44",
        "list.highlight": "#60cfff33",
        "list.hover": "#60cfff1a",

        "link_text.hover": "#7dd3fcff",

        "search.active_match_background": "#c79cea99",
        "search.match_background": "#5a557366",

        "scrollbar.thumb.active_background": "#60cfffcc",
        "scrollbar.thumb.background": "#5a5573aa",
        "scrollbar.thumb.hover_background": "#7a759aff",
        "scrollbar.track.background": "#00000000",
        "scrollbar.track.border": "#00000000",

        "version_control.added": "#3a8abfff",
        "version_control.conflict_marker.ours": "#60cfff44",
