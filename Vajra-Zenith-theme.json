        "version_control.deleted": "#c79ceaff",
        "version_control.modified": "#ffca80ff",
        "version_control.word_added": "#3a8abf55",
        "version_control.word_deleted": "#c79cea77",

        "icon": "#b5bae6ff",
        "icon.accent": "#60cfff",
        "icon.disabled": "#6e6a86ff",
        "icon.muted": "#9a95bbff",
        "icon.placeholder": "#9a95bbff",

        "terminal.ansi.black": "#1a1828ff",
        "terminal.ansi.blue": "#60cfff",
        "terminal.ansi.bright_black": "#5a5573ff",
        "terminal.ansi.bright_blue": "#8ad9ffff",
        "terminal.ansi.bright_cyan": "#7ef0ffff",
        "terminal.ansi.bright_green": "#4f9bd4ff",
        "terminal.ansi.bright_magenta": "#dec5ffff",
        "terminal.ansi.bright_red": "#d8b0f5ff",
        "terminal.ansi.bright_white": "#f5f2ffff",
        "terminal.ansi.bright_yellow": "#ffd599ff",
        "terminal.ansi.cyan": "#4fe8f9ff",
        "terminal.ansi.dim_black": "#1a1828ff",
        "terminal.ansi.dim_blue": "#60cfffcc",
        "terminal.ansi.dim_cyan": "#4fe8f9cc",
        "terminal.ansi.dim_green": "#3a8abfcc",
        "terminal.ansi.dim_magenta": "#d0b3facc",
        "terminal.ansi.dim_red": "#c79ceacc",
        "terminal.ansi.dim_white": "#f0edffcc",
        "terminal.ansi.dim_yellow": "#ffca80cc",
        "terminal.ansi.green": "#3a8abfff",
        "terminal.ansi.magenta": "#d0b3faff",
        "terminal.ansi.red": "#c79ceaff",
        "terminal.ansi.white": "#f0edffff",
        "terminal.ansi.yellow": "#ffca80ff",
        "terminal.background": "#211f32ff",
        "terminal.bright_foreground": "#f5f2ffff",
        "terminal.dim_foreground": "#9a95bbff",
        "terminal.foreground": "#f0edffff",

        "syntax": {
          "attribute": { "color": "#7dd3fcff" },
          "comment": { "color": "#7a759aff", "font_style": "italic" },
          "comment.doc": { "color": "#7a759aff", "font_style": "italic" },
          "constant": { "color": "#ffca80ff" },
          "constant.character": { "color": "#ffca80ff" },
          "constant.character.escape": { "color": "#ffca80ff" },
          "constant.numeric": { "color": "#ffca80ff" },
          "emphasis": { "color": "#d0b3faff", "font_style": "italic" },
          "entity.name.function": { "color": "#60cfff", "font_weight": 500 },
          "entity.name.tag": { "color": "#c79ceaff" },
          "entity.other.attribute-name": { "color": "#7dd3fcff" },
          "function": { "color": "#60cfff" },
          "function.method": { "color": "#60cfff" },
          "function.special": { "color": "#60cfff" },
          "keyword": { "color": "#d0b3faff" },
          "keyword.control": { "color": "#d0b3faff", "font_weight": 500 },
          "keyword.operator": { "color": "#60cfffcc" },
          "keyword.other.important": { "color": "#ff7b7bff" },
          "keyword.storage": { "color": "#c79ceaff" },
          "punctuation": { "color": "#b5bae6ff" },
          "punctuation.bracket": { "color": "#b5bae6ff" },
          "punctuation.delimiter": { "color": "#b5bae6ff" },
          "punctuation.list_marker": { "color": "#60cfff" },
          "punctuation.special": { "color": "#c79ceaff" },
          "property": { "color": "#4fe8f9ff" },
          "string": { "color": "#ffca80ff" },
          "string.escape": { "color": "#ffca80ff" },
          "string.regex": { "color": "#ffca80ff" },
          "string.special": { "color": "#ffca80ff" },
          "string.special.symbol": { "color": "#ffca80ff" },
          "storage.type": { "color": "#c79ceaff" },
          "support.function": { "color": "#60cfff" },
          "support.type": { "color": "#7dd3fcff" },
          "support.type.property-name": { "color": "#60cfff" },
          "tag": { "color": "#c79ceaff" },
          "text.literal": { "color": "#ffca80ff" },
          "title": { "color": "#60cfff", "font_weight": 700 },
          "type": { "color": "#7dd3fcff" },
          "type.enum": { "color": "#7dd3fcff" },
          "type.interface": { "color": "#7dd3fcff" },
          "type.struct": { "color": "#7dd3fcff" },
          "variable": { "color": "#f0edffff" },
          "variable.member": { "color": "#b5bae6ff" },
          "variable.parameter": {
            "color": "#b5bae6ff",
            "font_style": "italic"
          },
          "variable.special": { "color": "#c79ceaff" }
        },

        "players": [
          {
            "cursor": "#60cfff",
            "selection": "#60cfff44",
            "background": "#60cfff22"
          },
          {
            "cursor": "#d0b3fa",
            "selection": "#d0b3fa44",
            "background": "#d0b3fa22"
          },
          {
            "cursor": "#c79cea",
            "selection": "#c79cea44",
            "background": "#c79cea22"
          },
          {
            "cursor": "#3a8abf",
            "selection": "#3a8abf44",
            "background": "#3a8abf22"
          },
          {
            "cursor": "#ffca80",
            "selection": "#ffca8044",
            "background": "#ffca8022"
          },
          {
            "cursor": "#4fe8f9",
            "selection": "#4fe8f944",
            "background": "#4fe8f922"
          },
          {
            "cursor": "#b5bae6",
            "selection": "#b5bae644",
            "background": "#b5bae622"
          },
          {
            "cursor": "#f0edff",
            "selection": "#f0edff44",
            "background": "#f0edff22"
          }
        ]
      }
    }
  ]
}
